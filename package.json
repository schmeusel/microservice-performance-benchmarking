{
  "name": "microservice-performance-benchmarking",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/schmeusel/microservice-performance-benchmarking.git",
  "author": "Lukas <meusel.lukas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test-backend": "mocha --require ts-node/register ./tests/**/*.test.ts",
    "test-frontend": "jest",
    "build-server": "tsc",
    "build-client": "webpack --config webpack.config.js && cp ./src/webServer/public/index.html ./build/webServer/public/index.html",
    "build-client-watch": "cp ./src/webServer/public/index.html ./build/webServer/public/index.html && webpack --config webpack.config.js --watch",
    "build": "yarn run build-server && yarn run build-client",
    "start": "yarn run build && node ./build/app.js --json ./assets/sampleAPI_extended.json --spec ./assets/benchmarkSpec.json",
    "lint": "eslint --fix ./src/webServer/public/* ./tests/webServer/public/*"
  },
  "jest": {
    "verbose": true,
    "testRegex": "/tests/webServer/.*\\.test\\.(js)$",
    "transform": {
      ".js": "./node_modules/babel-jest"
    },
    "setupTestFrameworkScriptFile": "./tests/webServer/public/setup.js"
  },
  "dependencies": {
    "chart.js": "^2.7.2",
    "chartjs-chart-box-and-violin-plot": "^1.0.0",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "json-schema-faker": "^0.5.0-rc13",
    "material-ui": "^0.20.0",
    "minimist": "^1.2.0",
    "morgan": "^1.9.0",
    "prob.js": "^1.0.6",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "request": "^2.85.0",
    "simple-statistics": "^5.3.1",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "swagger-client": "^3.7.2",
    "typescript": "^2.8.1",
    "winston": "^3.0.0-rc3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.4.7",
    "@types/request": "^2.47.0",
    "@types/sinon": "^4.3.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "mocha": "^5.0.5",
    "proxyquire": "^2.0.1",
    "sinon": "^4.5.0",
    "ts-node": "^5.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  }
}
